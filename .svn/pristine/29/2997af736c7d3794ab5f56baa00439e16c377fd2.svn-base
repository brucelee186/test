package com.mtf.framework.model.impl;

import java.util.List;


import com.mtf.framework.model.Item;
import com.mtf.framework.model.Office;


public class OfficeImpl extends Office{
	
	// 开始日期
	private String startDate;
	// 结束日期
	private String endDate;
	// 合同货品列表
	private List<Item> listItem;

	// 当前员工如果为领导时,子员工的编号集合
	List<String> listEmployee;
	// 审批状态集合(saved:已保存, submit:已提交, reject1:一级驳回, reject2:二级驳回, approval1:一级审批完毕, approval2:二级审批完毕)
	List<String> listApproveState;


	// 用户等级 0 员工  1 部门经理
	private Integer userLevel;
	
	private String[] officeIds;
	
	
	// 显示添加时间
	private String showAddDate;
	
	// 显示部门审批时间
	private String showApprove1Date;
	
	// 显示行政部审批时间
	
	private String showApprove2Date;
	
	// 审批时间
	private String showModifiedDate;
	
	public String getShowModifiedDate() {
		return showModifiedDate;
	}



	public void setShowModifiedDate(String showModifiedDate) {
		this.showModifiedDate = showModifiedDate;
	}



	public String getStartDate() {
		return startDate;
	}


	
	public void setStartDate(String startDate) {
		this.startDate = startDate;
	}


	
	public String getEndDate() {
		return endDate;
	}


	
	public void setEndDate(String endDate) {
		this.endDate = endDate;
	}


	
	
	
	public List<Item> getListItem() {
		return listItem;
	}



	
	public void setListItem(List<Item> listItem) {
		this.listItem = listItem;
	}



	public List<String> getListEmployee() {
		return listEmployee;
	}


	
	public void setListEmployee(List<String> listEmployee) {
		this.listEmployee = listEmployee;
	}


	
	public List<String> getListApproveState() {
		return listApproveState;
	}


	
	public void setListApproveState(List<String> listApproveState) {
		this.listApproveState = listApproveState;
	}


	
	public Integer getUserLevel() {
		return userLevel;
	}


	
	public void setUserLevel(Integer userLevel) {
		this.userLevel = userLevel;
	}



	
	public String[] getOfficeIds() {
		return officeIds;
	}



	
	public void setOfficeIds(String[] officeIds) {
		this.officeIds = officeIds;
	}



	
	public String getShowAddDate() {
		return showAddDate;
	}



	
	public void setShowAddDate(String showAddDate) {
		this.showAddDate = showAddDate;
	}



	
	public String getShowApprove1Date() {
		return showApprove1Date;
	}



	
	public void setShowApprove1Date(String showApprove1Date) {
		this.showApprove1Date = showApprove1Date;
	}



	
	public String getShowApprove2Date() {
		return showApprove2Date;
	}



	
	public void setShowApprove2Date(String showApprove2Date) {
		this.showApprove2Date = showApprove2Date;
	}
	
	


}
