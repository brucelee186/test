package 线程.实现线程的几种方式;

import java.util.Timer;
import java.util.TimerTask;

public class TestTimer {
	public static void main(String[] args) {
		new Timer(){}.schedule(new TimerTask() {
			
			@Override
			public void run() {
				System.err.println("doSomething");
			}
		}, 5000);
	}
	
}
