package 构造器;

public class Student {
	public Student() {
		super();
	}
	
	protected String name;
	
	protected String getString(String name){
		return name + ": is a hamer";
	}
	
	protected int age;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

}
