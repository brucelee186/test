package framework.Spring.injection.constructon;

public class ConstructonInjectionPropertyBean {
	private String name;
	private int age;
	public String getName() {
		return name;
	}
	
	//使用有参构造,无参构造会被覆盖
	public ConstructonInjectionPropertyBean(String name, int age) {
		super();
		this.name = name;
		this.age = age;
	}

	//提供外部赋值取值访问
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	
}
