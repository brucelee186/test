package 测试.参数;

public class Test {
	public static void tripleValue(double x) {
		x = 3 * x;
		System.err.println("end method x = " + x);
	}
	
	public static void main(String[] args) {
		System.err.println("Test 1: Methods can't modify numeric parameters");
		double precent = 20.00;
		System.err.println("before precent = " + precent);
		tripleValue(precent);
		System.err.println("after precent = " + precent);
		System.err.println();
		System.err.println("Test 2: Methods can change the state of object parameters");
		
	}
}
